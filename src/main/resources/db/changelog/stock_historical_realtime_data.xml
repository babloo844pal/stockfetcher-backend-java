<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- Historical Data Table -->
    <changeSet id="5" author="author">
        <createTable tableName="historical_data">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="meta_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="interval_time" type="VARCHAR(10)">
                <constraints nullable="false"/>
            </column>
            <column name="datetime" type="TIMESTAMP"/>
            <column name="open" type="DECIMAL(15,5)"/>
            <column name="high" type="DECIMAL(15,5)"/>
            <column name="low" type="DECIMAL(15,5)"/>
            <column name="close" type="DECIMAL(15,5)"/>
            <column name="volume" type="BIGINT"/>
        </createTable>
        <addForeignKeyConstraint baseTableName="historical_data"
                                 baseColumnNames="meta_id"
                                 constraintName="fk_historical_meta"
                                 referencedTableName="meta_info"
                                 referencedColumnNames="id"/>
    </changeSet>
</databaseChangeLog>
